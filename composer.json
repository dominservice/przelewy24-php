{
    "name": "dominservice/przelewy24-php",
    "description": "Przelewy24 PHP library",
    "keywords": [
        "przelewy24",
        "payments",
        "Cards",
        "Charge card",
        "Płatności",
        "płatność kartą",
        "płatności rekurencyjne",
        "subskrypcja"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/dominservice/przelewy24-php",
    "authors": [
        {
            "name": "Mateusz Domin",
            "homepage": "https://dso.biz.pl",
            "email": "biuro@dso.biz.pl"
        }, {
            "name": "dbojdo",
            "email": "daniel@bojdo.eu"
        },{
            "name": "Mateusz Nastalski",
            "homepage": "https://nastalski.pl"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.6"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.34",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Przelewy24\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Przelewy24\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
